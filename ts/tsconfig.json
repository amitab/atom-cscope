{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "../dist",
    "rootDir": "./",
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  },
  "compileOnSave": true
}
